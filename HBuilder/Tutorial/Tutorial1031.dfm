    object Label33: TdhLabel
      Left = 22
      Top = 1720
      Width = 600
      Height = 277
      Text = 
        'Ein Label ist ein einfaches Objekt mit textuellem Inhalt.'#10'Text k' +
        'ann im <tab>&nbsp;&nbsp;&nbsp;Text&nbsp;&nbsp;&nbsp;&nbsp;</tab>' +
        '-Tab eingegeben werden oder'#10#252'ber den Texteditor-Dialog, der eine' +
        ' Formatierung des Textes gestattet: entweder '#252'ber '#252'bliche HTML-T' +
        'ags oder'#10#252'ber Stile, die in Stylesheet-Objekten definiert wurden' +
        '.'#10'Der Texteditor kann '#252'ber das Kontextmenu des Objekts ge'#246'ffnet ' +
        'werden oder aber indem du auf das Objekt klickst w'#228'hrend du die ' +
        'ALT-Taste gedr'#252'ckt h'#228'ltst.In diesem Fall wird der Cursor im Edit' +
        'or auf die Position gesetzt, die mit der Maus anvisiert wurde.'#10'<' +
        'br/><br/>'#10'Die Formatierung eines Textteils wird durch ein Startt' +
        'ag der Form <b><code>&lt;X&gt;</code></b>'#10'eingeleitet und durch ' +
        'ein Endtag der Form <b><code>&lt;/X&gt;</code></b> abgeschlossen' +
        '.'#10'(Im Gegensatz zu HTML sind keine Attribute gestattet.)'#10'Ein Tag' +
        '-Paar kann '#252'ber die <Label34>UNTAG</Label34>-Schaltfl'#228'che entfer' +
        'nt werden.'#10'<br/><br/>'#10'Beliebige Unicode-Zeichen, die aber von de' +
        'r gew'#228'hlten Schriftart unterst'#252'tzt werden m'#252'ssen, k'#246'nnen '#252'ber di' +
        'e <Label34>UNICODE</Label34>-Schaltfl'#228'che eingef'#252'gt werden.'#10'Da z' +
        '.B. die Zeichen <b>&lt;</b> und <b>&gt;</b> schon zum Markieren ' +
        'von Start- und Endtags genutzt werden,'#10'k'#246'nnen die speziellen Zei' +
        'chen <b>&lt;</b>, <b>&gt;</b>, <b>&quot;</b> and <b>&amp;</b> in' +
        ' selektiertem Text '#252'ber die <Label34>MASK</Label34>-Schaltfl'#228'che' +
        #10'maskiert werden.'#10#10'Ein Zeilenumbruch kann '#252'ber die <Label34>BR</' +
        'Label34>-Schaltfl'#228'che eingef'#252'gt werden.'
      Use = pad
      AutoSizeXY = asY
    end
